{
    "name": "Shortcut Search",
    "description": "aramaları kolaylaştırır!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "host_permissions": ["https://www.youtube.com/*"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "ss.jpg"
    }
}